class Solution {
    public String removeDuplicates(String s) {
        //ArrayDeque会比LinkedList在除了删除元素这一点外会快一点
         ArrayDeque<Character> deque = new ArrayDeque<>(); //使用 Deque 作为堆栈
         char ch;
         for(int i = 0; i< s.length(); i++){
             ch = s.charAt(i);
             if(deque.isEmpty() || deque.peek()!=ch){
                 deque.push(ch);
             }else{
                 deque.pop();
             }
         }
         String str = "";
         //剩下的元素都是不重复的
         while(!deque.isEmpty()){
             str = deque.pop() + str;
         }
         return str;

    }
}
